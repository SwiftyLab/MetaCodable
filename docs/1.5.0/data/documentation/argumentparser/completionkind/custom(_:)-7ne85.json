{"variants":[{"paths":["\/documentation\/argumentparser\/completionkind\/custom(_:)-7ne85"],"traits":[{"interfaceLanguage":"swift"}]}],"hierarchy":{"paths":[["doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser","doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser\/CompletionKind"]]},"kind":"symbol","schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser\/CompletionKind\/custom(_:)-7ne85","interfaceLanguage":"swift"},"sections":[],"metadata":{"modules":[{"name":"ArgumentParser"}],"externalID":"s:14ArgumentParser14CompletionKindV6customyACSaySSGAE_SiSStcFZ","symbolKind":"method","title":"custom(_:)","roleHeading":"Type Method","fragments":[{"kind":"keyword","text":"static"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"custom","kind":"identifier"},{"kind":"text","text":"((["},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"kind":"text","text":"], "},{"preciseIdentifier":"s:Si","text":"Int","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:SS","text":"String"},{"text":") -> [","kind":"text"},{"preciseIdentifier":"s:SS","text":"String","kind":"typeIdentifier"},{"kind":"text","text":"]) -> "},{"text":"CompletionKind","kind":"typeIdentifier","preciseIdentifier":"s:14ArgumentParser14CompletionKindV"}],"role":"symbol"},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"tokens":[{"text":"@preconcurrency","kind":"attribute"},{"text":" ","kind":"text"},{"text":"static","kind":"keyword"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"custom"},{"kind":"text","text":"("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"text":"completion","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"@escaping","kind":"attribute"},{"text":" ([","kind":"text"},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":"], "},{"text":"Int","preciseIdentifier":"s:Si","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":") -> [","kind":"text"},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":"]) -> "},{"text":"CompletionKind","kind":"typeIdentifier","identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser\/CompletionKind","preciseIdentifier":"s:14ArgumentParser14CompletionKindV"}],"platforms":["macOS"]}]},{"kind":"content","content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"Completion candidates are interpreted by the requesting shell as literals."},{"text":" ","type":"text"},{"type":"text","text":"They must be neither escaped nor quoted; Swift Argument Parser escapes or"},{"text":" ","type":"text"},{"type":"text","text":"quotes them as necessary for the requesting shell."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The given closure is evaluated after a user invokes completion in their"},{"text":" ","type":"text"},{"type":"text","text":"shell (normally by pressing TAB); it is not evaluated when a completion"},{"type":"text","text":" "},{"text":"script is generated.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"The array of strings passed to the given closure contains all the shell","type":"text"},{"type":"text","text":" "},{"text":"words in the command line for the current command at completion","type":"text"},{"type":"text","text":" "},{"text":"invocation; this is exclusive of words for prior or subsequent commands or","type":"text"},{"type":"text","text":" "},{"type":"text","text":"pipes, but inclusive of redirects and any other command line elements."},{"type":"text","text":" "},{"type":"text","text":"Each word is its own element in the argument array; they appear in the"},{"type":"text","text":" "},{"text":"same order as in the command line. Note that shell words may contain","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"spaces if they are escaped or quoted."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Shell words are passed to Swift verbatim, without processing or removing"},{"text":" ","type":"text"},{"type":"text","text":"any quotes or escapes. For example, the shell word "},{"code":"\"abc\\\\\"\"def\"","type":"codeVoice"},{"text":" would be","type":"text"},{"type":"text","text":" "},{"text":"passed to Swift as ","type":"text"},{"code":"\"abc\\\\\"\"def\"","type":"codeVoice"},{"text":" (i.e. the Swift String’s contents would","type":"text"},{"type":"text","text":" "},{"text":"include all 4 of the double quotes and the 2 consecutive backslashes).","type":"text"}]},{"inlineContent":[{"type":"text","text":"The second argument (an "},{"type":"codeVoice","code":"Int"},{"text":") is the 0-based index of the word for which","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"completions are being requested within the given "},{"type":"codeVoice","code":"[String]"},{"type":"text","text":"."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The third argument (a "},{"type":"codeVoice","code":"String"},{"type":"text","text":") is the prefix of the word for which"},{"type":"text","text":" "},{"text":"completions are being requested that precedes the cursor.","type":"text"}]},{"level":3,"anchor":"bash","text":"bash","type":"heading"},{"inlineContent":[{"text":"In bash 3-, a process substitution (","type":"text"},{"code":"<(…)","type":"codeVoice"},{"type":"text","text":") in the command line prevents"},{"text":" ","type":"text"},{"type":"text","text":"Swift custom completion functions from being called."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"In bash 4+, a process substitution (","type":"text"},{"code":"<(…)","type":"codeVoice"},{"type":"text","text":") is split into multiple"},{"type":"text","text":" "},{"type":"text","text":"elements in the argument array: one for the starting "},{"code":"<(","type":"codeVoice"},{"type":"text","text":", and one for"},{"type":"text","text":" "},{"type":"text","text":"each unescaped\/unquoted-space-separated token through the closing "},{"code":")","type":"codeVoice"},{"type":"text","text":"."}]},{"inlineContent":[{"text":"In bash, if the cursor is between the backslash and the single quote for","type":"text"},{"text":" ","type":"text"},{"text":"the last escaped single quote in a word, all subsequent pipes or other","type":"text"},{"type":"text","text":" "},{"type":"text","text":"commands are included in the words passed to Swift. This oddity might"},{"text":" ","type":"text"},{"text":"occur only when additional constraints are met. This or similar oddities","type":"text"},{"type":"text","text":" "},{"type":"text","text":"might occur in other circumstances."}],"type":"paragraph"},{"level":3,"type":"heading","anchor":"fish","text":"fish"},{"type":"paragraph","inlineContent":[{"text":"In fish 3-, due to a bug, the argument array includes the fish words only","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"through the word being completed. This is fixed in fish 4+."}]},{"inlineContent":[{"text":"In fish, a redirect’s symbol is not included, but its source\/target is.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"In fish 3-, due to limitations, words are passed to Swift unquoted. For"},{"text":" ","type":"text"},{"text":"example, the shell word ","type":"text"},{"type":"codeVoice","code":"\"abc\\\\\"\"def\""},{"type":"text","text":" would be passed to Swift as"},{"text":" ","type":"text"},{"type":"codeVoice","code":"abc\\def"},{"text":". This is fixed in fish 4+.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"In fish 3-, the cursor index is provided based on the verbatim word, not"},{"text":" ","type":"text"},{"text":"based on the unquoted word, so it can be inconsistent with the unquoted","type":"text"},{"type":"text","text":" "},{"text":"word that is supplied to Swift. This problem does not exist in fish 4+.","type":"text"}]},{"level":3,"text":"zsh","anchor":"zsh","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"In zsh, redirects (both their symbol and source\/target) are omitted.","type":"text"}]},{"inlineContent":[{"text":"In zsh, if the cursor is between a backslash and the character that it","type":"text"},{"type":"text","text":" "},{"text":"escapes, the shell cursor index will be indicated as after the escaped","type":"text"},{"type":"text","text":" "},{"type":"text","text":"character, not as after the backslash."}],"type":"paragraph"}]}],"abstract":[{"text":"The completion candidates are the strings in the array returned by the","type":"text"},{"text":" ","type":"text"},{"text":"given closure when it is executed in response to a user’s request for","type":"text"},{"type":"text","text":" "},{"type":"text","text":"completions."}],"references":{"doc://com.SwiftyLab.HelperCoders/documentation/ArgumentParser/CompletionKind/custom(_:)-7ne85":{"url":"\/documentation\/argumentparser\/completionkind\/custom(_:)-7ne85","kind":"symbol","title":"custom(_:)","identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser\/CompletionKind\/custom(_:)-7ne85","fragments":[{"kind":"keyword","text":"static"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"custom"},{"kind":"text","text":"((["},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":"], ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:Si","text":"Int"},{"kind":"text","text":", "},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"kind":"text","text":") -> ["},{"text":"String","preciseIdentifier":"s:SS","kind":"typeIdentifier"},{"kind":"text","text":"]) -> "},{"kind":"typeIdentifier","text":"CompletionKind","preciseIdentifier":"s:14ArgumentParser14CompletionKindV"}],"type":"topic","role":"symbol","abstract":[{"text":"The completion candidates are the strings in the array returned by the","type":"text"},{"text":" ","type":"text"},{"text":"given closure when it is executed in response to a user’s request for","type":"text"},{"type":"text","text":" "},{"type":"text","text":"completions."}]},"doc://com.SwiftyLab.HelperCoders/documentation/ArgumentParser":{"kind":"symbol","role":"collection","url":"\/documentation\/argumentparser","abstract":[],"title":"ArgumentParser","identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser","type":"topic"},"doc://com.SwiftyLab.HelperCoders/documentation/ArgumentParser/CompletionKind":{"kind":"symbol","type":"topic","navigatorTitle":[{"text":"CompletionKind","kind":"identifier"}],"title":"CompletionKind","identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/ArgumentParser\/CompletionKind","abstract":[{"text":"The type of completion to use for an argument or option value.","type":"text"}],"url":"\/documentation\/argumentparser\/completionkind","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"text":"CompletionKind","kind":"identifier"}]}}}