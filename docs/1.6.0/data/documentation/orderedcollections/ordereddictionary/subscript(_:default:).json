{"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/orderedcollections\/ordereddictionary\/subscript(_:default:)"]}],"hierarchy":{"paths":[["doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/OrderedCollections","doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/OrderedCollections\/OrderedDictionary"]]},"sections":[],"schemaVersion":{"patch":0,"minor":3,"major":0},"metadata":{"symbolKind":"subscript","role":"symbol","title":"subscript(_:default:)","extendedModule":"OrderedCollections","externalID":"s:18OrderedCollections0A10DictionaryV_7defaultq_x_q_yXKtcip","fragments":[{"kind":"keyword","text":"subscript"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"Key","preciseIdentifier":"s:18OrderedCollections0A10DictionaryV3Keyxmfp"},{"kind":"text","text":", "},{"kind":"externalParam","text":"default"},{"kind":"text","text":" _: "},{"kind":"attribute","text":"@autoclosure"},{"kind":"text","text":" () -> "},{"kind":"typeIdentifier","text":"Value","preciseIdentifier":"s:18OrderedCollections0A10DictionaryV5Valueq_mfp"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Value","preciseIdentifier":"s:18OrderedCollections0A10DictionaryV5Valueq_mfp"}],"roleHeading":"Instance Subscript","conformance":{"availabilityPrefix":[{"text":"Available when","type":"text"}],"conformancePrefix":[{"text":"Conforms when","type":"text"}],"constraints":[{"code":"Key","type":"codeVoice"},{"text":" conforms to ","type":"text"},{"code":"Hashable","type":"codeVoice"},{"text":".","type":"text"}]},"modules":[{"name":"OrderedCollections"}]},"kind":"symbol","abstract":[{"type":"text","text":"Accesses the value with the given key. If the dictionary doesn’t contain"},{"type":"text","text":" "},{"type":"text","text":"the given key, accesses the provided default value as if the key and"},{"type":"text","text":" "},{"type":"text","text":"default value existed in the dictionary."}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/OrderedCollections\/OrderedDictionary\/subscript(_:default:)"},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"tokens":[{"text":"subscript","kind":"keyword"},{"text":"(","kind":"text"},{"text":"key","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"Key","kind":"typeIdentifier","preciseIdentifier":"s:18OrderedCollections0A10DictionaryV3Keyxmfp"},{"text":", ","kind":"text"},{"text":"default","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"defaultValue","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"@autoclosure","kind":"attribute"},{"text":" () -> ","kind":"text"},{"text":"Value","kind":"typeIdentifier","preciseIdentifier":"s:18OrderedCollections0A10DictionaryV5Valueq_mfp"},{"text":") -> ","kind":"text"},{"text":"Value","kind":"typeIdentifier","preciseIdentifier":"s:18OrderedCollections0A10DictionaryV5Valueq_mfp"},{"text":" { ","kind":"text"},{"text":"get","kind":"keyword"},{"text":" ","kind":"text"},{"text":"set","kind":"keyword"},{"text":" }","kind":"text"}],"languages":["swift"]}]},{"kind":"parameters","parameters":[{"name":"key","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The key the look up in the dictionary."}]}]},{"name":"defaultValue","content":[{"inlineContent":[{"type":"text","text":"The default value to use if "},{"type":"codeVoice","code":"key"},{"type":"text","text":" doesn’t exist in the"},{"type":"text","text":" "},{"type":"text","text":"dictionary."}],"type":"paragraph"}]}]},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"return-value","text":"Return Value"},{"inlineContent":[{"type":"text","text":"The value associated with "},{"type":"codeVoice","code":"key"},{"type":"text","text":" in the dictionary; otherwise,"},{"type":"text","text":" "},{"type":"codeVoice","code":"defaultValue"},{"type":"text","text":"."}],"type":"paragraph"}]},{"kind":"content","content":[{"anchor":"overview","level":2,"text":"Overview","type":"heading"},{"inlineContent":[{"type":"text","text":"Use this subscript when you want either the value for a particular key"},{"type":"text","text":" "},{"type":"text","text":"or, when that key is not present in the dictionary, a default value. This"},{"type":"text","text":" "},{"type":"text","text":"example uses the subscript with a message to use in case an HTTP response"},{"type":"text","text":" "},{"type":"text","text":"code isn’t recognized:"}],"type":"paragraph"},{"code":["var responseMessages: OrderedDictionary = [","    200: \"OK\",","    403: \"Access forbidden\",","    404: \"File not found\",","    500: \"Internal server error\"]","","let httpResponseCodes = [200, 403, 301]","for code in httpResponseCodes {","    let message = responseMessages[code, default: \"Unknown response\"]","    print(\"Response \\(code): \\(message)\")","}","\/\/ Prints \"Response 200: OK\"","\/\/ Prints \"Response 403: Access forbidden\"","\/\/ Prints \"Response 301: Unknown response\""],"syntax":null,"type":"codeListing"},{"inlineContent":[{"text":"When a dictionary’s ","type":"text"},{"type":"codeVoice","code":"Value"},{"text":" type has value semantics, you can use this","type":"text"},{"text":" ","type":"text"},{"text":"subscript to perform in-place operations on values in the dictionary.","type":"text"},{"text":" ","type":"text"},{"text":"The following example uses this subscript while counting the occurrences","type":"text"},{"text":" ","type":"text"},{"text":"of each letter in a string:","type":"text"}],"type":"paragraph"},{"code":["let message = \"Hello, Elle!\"","var letterCounts: OrderedDictionary<Character, Int> = [:]","for letter in message {","    letterCounts[letter, default: 0] += 1","}","\/\/ letterCounts == [\"H\": 1, \"e\": 2, \"l\": 4, \"o\": 1, ...]"],"syntax":null,"type":"codeListing"},{"inlineContent":[{"type":"text","text":"When "},{"code":"letterCounts[letter, defaultValue: 0] += 1","type":"codeVoice"},{"type":"text","text":" is executed with a"},{"type":"text","text":" "},{"type":"text","text":"value of "},{"code":"letter","type":"codeVoice"},{"type":"text","text":" that isn’t already a key in "},{"code":"letterCounts","type":"codeVoice"},{"type":"text","text":", the"},{"type":"text","text":" "},{"type":"text","text":"specified default value ("},{"code":"0","type":"codeVoice"},{"type":"text","text":") is returned from the subscript,"},{"type":"text","text":" "},{"type":"text","text":"incremented, and then added to the dictionary under that key."}],"type":"paragraph"},{"style":"note","name":"Note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Do not use this subscript to modify dictionary values if the"},{"type":"text","text":" "},{"type":"text","text":"dictionary’s "},{"type":"codeVoice","code":"Value"},{"type":"text","text":" type is a class. In that case, the default value"},{"type":"text","text":" "},{"type":"text","text":"and key are not written back to the dictionary after an operation. (For"},{"type":"text","text":" "},{"type":"text","text":"a variant of this operation that supports this usecase, see"},{"type":"text","text":" "},{"type":"codeVoice","code":"updateValue(forKey:default:_:)"},{"type":"text","text":".)"}]}],"type":"aside"},{"style":"note","name":"Complexity","content":[{"inlineContent":[{"type":"text","text":"Looking up values in the dictionary through this subscript"},{"type":"text","text":" "},{"type":"text","text":"has an expected complexity of O(1) hashing\/comparison operations on"},{"type":"text","text":" "},{"type":"text","text":"average, if "},{"type":"codeVoice","code":"Key"},{"type":"text","text":" implements high-quality hashing. Updating the"},{"type":"text","text":" "},{"type":"text","text":"dictionary also has an amortized expected complexity of O(1) –"},{"type":"text","text":" "},{"type":"text","text":"although individual updates may need to copy or resize the dictionary’s"},{"type":"text","text":" "},{"type":"text","text":"underlying storage."}],"type":"paragraph"}],"type":"aside"}]}],"references":{"doc://com.SwiftyLab.HelperCoders/documentation/OrderedCollections/OrderedDictionary":{"identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/OrderedCollections\/OrderedDictionary","role":"symbol","kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrderedDictionary"}],"abstract":[{"type":"text","text":"An ordered collection of key-value pairs."}],"title":"OrderedDictionary","type":"topic","url":"\/documentation\/orderedcollections\/ordereddictionary","navigatorTitle":[{"kind":"identifier","text":"OrderedDictionary"}]},"doc://com.SwiftyLab.HelperCoders/documentation/OrderedCollections":{"identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/OrderedCollections","title":"OrderedCollections","type":"topic","kind":"symbol","url":"\/documentation\/orderedcollections","role":"collection","abstract":[]},"doc://com.SwiftyLab.HelperCoders/documentation/OrderedCollections/OrderedDictionary/subscript(_:default:)":{"title":"subscript(_:default:)","url":"\/documentation\/orderedcollections\/ordereddictionary\/subscript(_:default:)","fragments":[{"text":"subscript","kind":"keyword"},{"text":"(","kind":"text"},{"preciseIdentifier":"s:18OrderedCollections0A10DictionaryV3Keyxmfp","text":"Key","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"default","kind":"externalParam"},{"text":" _: ","kind":"text"},{"text":"@autoclosure","kind":"attribute"},{"text":" () -> ","kind":"text"},{"preciseIdentifier":"s:18OrderedCollections0A10DictionaryV5Valueq_mfp","text":"Value","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"preciseIdentifier":"s:18OrderedCollections0A10DictionaryV5Valueq_mfp","text":"Value","kind":"typeIdentifier"}],"role":"symbol","type":"topic","kind":"symbol","abstract":[{"text":"Accesses the value with the given key. If the dictionary doesn’t contain","type":"text"},{"text":" ","type":"text"},{"text":"the given key, accesses the provided default value as if the key and","type":"text"},{"text":" ","type":"text"},{"text":"default value existed in the dictionary.","type":"text"}],"identifier":"doc:\/\/com.SwiftyLab.HelperCoders\/documentation\/OrderedCollections\/OrderedDictionary\/subscript(_:default:)","conformance":{"constraints":[{"code":"Key","type":"codeVoice"},{"type":"text","text":" conforms to "},{"code":"Hashable","type":"codeVoice"},{"type":"text","text":"."}],"conformancePrefix":[{"type":"text","text":"Conforms when"}],"availabilityPrefix":[{"type":"text","text":"Available when"}]}}}}